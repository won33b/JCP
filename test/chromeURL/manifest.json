{
  "manifest_version": 2,

  "name": "GET URL",
  "description": "GET URL",
  "version": "1.0.0",

  
  "web_accessible_resources": [
    "js/*" 
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup2.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ]
  
  
}